            app:layout_constraintHorizontal_bias="0.175"
            app:layout_constraintStart_toStartOf="@+id/doaa_corona"
            app:layout_constraintTop_toTopOf="@+id/adyaa_mnwaa"
            app:srcCompat="@drawable/covid" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="77dp"
            android:layout_height="46dp"
            android:layout_marginTop="60dp"
            android:gravity="center"
            android:text="دعاء التحصن من كورونا"
            android:textColor="#7B7C7C"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/doaa_corona"
            app:layout_constraintHorizontal_bias="0.365"
            app:layout_constraintStart_toStartOf="@+id/doaa_corona"
