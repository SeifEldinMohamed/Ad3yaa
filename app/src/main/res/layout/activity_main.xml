        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="63dp"
            android:layout_height="70dp"
            android:layout_marginTop="4dp"
            app:layout_constraintEnd_toEndOf="@+id/doaa_eat"
            app:layout_constraintHorizontal_bias="1.0"
